{
  "name": "ember-frost-chart",
  "version": "7.0.0",
  "description": "Composable charts with decoupled axes / plots and some extra pizzazz",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "lint:js": "eslint ./*.js addon addon-test-support app blueprints config lib server test-support tests",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-frost/ember-frost-chart.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Steven Glanzer (https://github.com/sglanzer)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.7.0",
    "chai-jquery": "^2.0.0",
    "ember-cli": "3.2.0-beta.1",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-code-coverage": "1.0.0-beta.3",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-frost-blueprints": "^5.0.2",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "^0.15.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-snippet": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-frost-test": "^4.0.1",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^2.1.0",
    "ember-source": "~3.2.0-beta.2",
    "ember-source-channel-url": "^1.0.1",
    "ember-test-utils": "^8.1.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "^4.2.3",
    "sinon-chai": "^3.0.0"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-sass": "^7.1.7",
    "ember-computed-decorators": "0.3.0",
    "ember-concurrency": "^0.8.17",
    "ember-d3": "^0.4.4",
    "ember-frost-core": "^8.0.0",
    "ember-hook": "^1.4.2",
    "ember-prop-types": "^7.0.2",
    "ember-resize": "github:ciena-blueplanet/ember-resize#d702c2e035416cba64197bafc48c14af300c52b4"
  },
  "ember-addon": {
    "after": [
      "ember-computed-decorators"
    ],
    "configPath": "tests/dummy/config"
  }
}
